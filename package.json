{
  "name": "isomorphic-hot-reloading-boilerplate",
  "sideEffects": false,
  "version": "1.0.0",
  "repository": "https://github.com/RonanQuigley/isomorphic-hot-reload-boilerplate.git",
  "author": "ronanquigley <info@ronanquigley.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@loadable/component": "^5.13.1",
    "@loadable/server": "^5.13.1",
    "apollo-boost": "^0.4.4",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.13",
    "apollo-server-express": "^2.9.13",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-universal-import": "^4.0.0",
    "chrome-launcher": "^0.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.4.7",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "eslint": "^7.4.0",
    "express": "^4.16.3",
    "express-static-gzip": "^2.0.5",
    "graphql": "^15.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.11",
    "morgan": "^1.9.0",
    "node-fetch": "^2.6.0",
    "node-mocks-http": "^1.7.0",
    "npm-run-all": "^4.1.2",
    "pg": "^8.3.0",
    "pino": "^6.3.2",
    "pino-pretty": "^4.0.1",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-universal-component": "^4.0.0",
    "styled-components": "^5.1.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-flush-chunks": "^2.0.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-log": "^3.0.1",
    "webpack-merge": "^5.0.9",
    "webpack-node-externals": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "7.10.4",
    "@babel/register": "7.10.4",
    "@loadable/babel-plugin": "^5.13.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.3",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.10.0",
    "lodash-webpack-plugin": "^0.11.5",
    "react-refresh": "^0.8.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-stats-plugin": "^0.3.0"
  },
  "scripts": {
    "create-migration": "db-migrate create --sql-file --config ./src/database/config.json --migrations-dir ./src/database/migrations",
    "db-down": "db-migrate down --config ./src/database/config.json --migrations-dir ./src/database/migrations -e dev",
    "db-up": "db-migrate up --config ./src/database/config.json --migrations-dir ./src/database/migrations -e dev",
    "build:all": "cross-env NODE_ENV=production node -r @babel/register ./webpack/webpack",
    "build:all:start": "npm-run-all -s build:all start:prod",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production npm-run-all build:all",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "start": "cross-env NODE_ENV=development node --max-old-space-size=8192 --inspect -r @babel/register ./src/server/app.js",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/RonanQuigley/isomorphic-hot-reload-boilerplate/issues"
  },
  "homepage": "https://github.com/RonanQuigley/isomorphic-hot-reload-boilerplate#readme"
}
