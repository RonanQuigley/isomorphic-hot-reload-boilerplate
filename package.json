  "main": "index.js",
  "repository": "https://github.com/RonanQuigley/isomorphic-hot-reload-boilerplate.git",
  "author": "ronanquigley <info@ronanquigley.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-react": "7.0.0-beta.40",
    "@babel/preset-stage-0": "7.0.0-beta.40",
    "@babel/preset-stage-1": "7.0.0-beta.40",
    "@babel/preset-stage-2": "7.0.0-beta.40",
    "@babel/preset-stage-3": "7.0.0-beta.40",
    "@babel/register": "7.0.0-beta.40",
    "@babel/runtime": "7.0.0-beta.40",
    "babel-loader": "^8.0.0-beta.0",
    "cache-loader": "^1.2.2",
    "chokidar": "^2.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "express": "^4.16.3",
    "express-generator": "^4.15.5",
    "express-handlebars": "^3.0.0",
    "handlebars": "^4.0.11",
    "happypack": "^5.0.0-beta.1",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-webpack-plugin": "^3.0.6",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.2",
    "opn": "^5.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "source-map-support": "^0.5.4",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "build:back-end:dev": "cross-env DEV_BUILD=true webpack --config webpack/back-end/webpack.dev.babel.js",
    "build:back-end:prod": "cross-env DEV_BUILD=true webpack --config webpack/back-end/webpack.prod.babel.js",
    "build:front-end:dev": "cross-env DEV_BUILD=true webpack --config webpack/front-end/webpack.dev.babel.js",
    "build:front-end:prod": "cross-env NODE_ENV=production DEV_BUILD=false webpack -p --config webpack/front-end/webpack.prod.babel.js",
    "build:full-stack:dev": "cross-env DEV_BUILD=true webpack --config webpack/full-stack/webpack.dev.babel.js",
    "build:full-stack:prod": "cross-env DEV_BUILD=true webpack --config webpack/full-stack/webpack.prod.babel.js",  
    "start:dev-build": "npm-run-all -s build:full-stack:dev start:build",
    "start:prod-build": "npm-run-all -s build:full-stack:prod start:build",
    "start:build": "node ./build/server.js",
    "start:dev": "cross-env NODE_ENV=development DEBUG=true ./node_modules/.bin/babel-node ./index.js",
    "start:prod": "cross-env NODE_ENV=production DEBUG=true ./node_modules/.bin/babel-node ./index.js",
    "start": "cross-env NODE_ENV=development babel-node ./src/index.js",
    "webpack": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.server.js"
  },
  "bugs": {
    "url": "https://github.com/RonanQuigley/isomorphic-hot-reload-boilerplate/issues"
  },
  "homepage": "https://github.com/RonanQuigley/isomorphic-hot-reload-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/node": "7.0.0-beta.40",
    "cross-env": "^5.1.4",
    "handlebars-loader": "https://github.com/Waavi/handlebars-loader",
    "html-loader": "^0.5.5",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
